{
    "model": {
        "type": "PairYOLOFeature",
        "backbone": {
            "out_levels": [
                4
            ],
            "in_channels": 3
        },
        "bbox_head": {
            "type": "YOLOFeatureHead",
            "in_channels": 256,
            "block_dilations": [2, 4, 6, 8],
            "stacked_residual_blocks": 4,
            "reg_num_convs": 4,
            "cls_num_convs": 2,
            "num_classes": 6,
            "anchor_generator": {
                "type":"AnchorGenerator",
                "octave_base_scale": 4,
                "scales_per_octave": 1,
                "ratios": [1.0, 0.5, 2.0],
                "strides": [16]
            },
            "bbox_coder": {
                "type":"DeltaXYWHBBoxCoder",
                "target_means":[0.0, 0.0, 0.0, 0.0],
                "target_stds":[1.0, 1.0, 1.0, 1.0]
            },
            "loss_cls": {
                "type":"FocalLoss",
                "use_sigmoid": true,
                "gamma": 2.0,
                "alpha": 0.25,
                "loss_weight": 1.0
            },
            "loss_bbox": {
                "type": "GIoULoss",
                "loss_weight": 1.0
            }
        }
    },
    "train_cfg": {
        "assigner": {"type":"UniformAssigner",
                "pos_ignore_thr": 0.15,
                "neg_ignore_thr": 0.7,
                "match_times": 4
        },
                "iou_calculator": {"type":"BboxOverlaps2D"},
        "sampler_cfg": {"type":"PseudoSampler",
                        "sampling": false
        },
        "allowed_border": -1,
        "pos_weight": -1,
        "debug": false,
        "stage_loss_weights": [1.0, 1.0],
        "num_anchors": 3,
        "auto_anchors": false

    },
    "test_cfg": {
        "nms_pre": 100,
        "score_thr": 0.5,
        "max_per_img": 100,
        "nms": {"type": "nms",
                     "iou_thr": 0.3
                    },
        "merge_cfg": {
             "nms_pre": 100,
             "score_thr": 0.5,
             "nms": {"type": "nms",
                     "iou_thr": 0.3
                    },
            "max_per_img": 100
            }
    }
}
