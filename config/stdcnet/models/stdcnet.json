{
    "model": {
        "type": "EncoderDecoder",
        "backbone": {
            "type": "STDCNet813",
            "use_conv_last": true
        },
        "decode_head": {
            "type": "STDCHead",
            "in_channels": 1024,
            "head_width": 64,
            "num_classes": 1,
            "loss": {"type": "OhemCELoss",
                    "thresh":  0.7,
                    "n_min": 65536
            }
        },
        "auxiliary_head": [
            {
                "type": "FCNHead",
                "in_channels":1024,
                "num_classes": 1,
                "num_convs": 1,
                "head_width": 64,
                "in_index": -1,
                "loss":  [{"type": "CrossEntropyLoss", "use_sigmoid": false ,"loss_weight": 1.0}]
            }]
    },
    "train_cfg": {},
    "test_cfg": {"mode": "whole"}
}

