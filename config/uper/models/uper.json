{
    "model": {
        "type": "EncoderDecoder",
        "backbone": {
            "type": "ConvNext",
            "use_conv_last": true
        },
        "decode_head": {
            "type": "UPerHead",
            "in_channels": [192, 384, 768],
            "head_width": 64,
            "num_classes": 1,
            "loss": {"type": "CrossEntropyLoss",
                     "use_sigmoid": false ,
                     "loss_weight": 1.0
            }
        },
        "auxiliary_head": [
            {
                "type": "FCNHead",
                "in_channels":768,
                "num_classes": 1,
                "num_convs": 1,
                "head_width": 64,
                "in_index": -1,
                "loss":  {"type": "CrossEntropyLoss", "use_sigmoid": false ,"loss_weight": 1.0}
            }],
                "siamese_layer": {
            "type": "PixelSub",
            "fea_list": [192, 384, 768],
            "in_c": 192,
            "ou_c": 1
        }
    },
    "train_cfg": {},
    "test_cfg": {"mode": "whole"}
}

